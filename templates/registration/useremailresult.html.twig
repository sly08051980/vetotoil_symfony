{% extends 'base.html.twig' %}

{% block title %}Register
{% endblock %}

{% block body %}





{% if user is not null %}
    <h1>User Found</h1>
    <p>Email: {{ user.getEmail() }}</p>
    <p>Nom : {{ user.getNom() }}</p>
    <p>Prenom : {{ user.getPrenom }}</p>
 
{% else %}
    <h1>User Not Found</h1>
{% endif %}

{% endblock %}